{
  "name": "Test",
  "description": "Leonov Nikolay",
  "devDependencies": {
    "autoprefixer": "9.5.x",
    "browser-sync": "2.26.x",
    "eslint": "4.16.0",
    "gulp": "4.0.x",
    "gulp-csso": "3.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "3.0.5",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "4.0.1",
    "posthtml-include": "^1.2.0",
    "stylelint": "13.13.1"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "eslint": "eslint source/js/scripts/*.js",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "gulp build && webpack --mode production",
    "start": "gulp start && webpack serve --mode development --open"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "14.20.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.18.2",
    "babel-loader": "^8.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.2",
    "del": "^4.1.1",
    "gulp-cached": "^1.1.1",
    "gulp-pug": "^5.0.0",
    "gulp-webpack": "^1.5.0",
    "gulp-webpack-config": "^3.2.0",
    "swiper": "^8.3.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-stream": "^7.0.0"
  }
}
